{
  "name": "abb-mf-remote",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "mock": "cd ./mock-server && yarn run dev",
    "build": "webpack --mode production",
    "start": "webpack-cli serve --mode development",
    "start-mock": "(npm run mock) | (npm start)",
    "test": "jest --ci --reporters=default --reporters=jest-junit --config jest.config.json",
    "test:watch": "jest --watchAll --config jest.config.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "format": "prettier --write '**/*.{ts,tsx,scss,css,js,jsx,json}'",
    "format-prettier": "prettier --eslint-config-path .eslintrc.js --write '**/*.{ts,tsx,scss,css,js,jsx,json}'"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.6.2",
    "big.js": "6.2.1",
    "framer-motion": "^10.16.16",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-date-picker": "^10.6.0",
    "react-datepicker": "^4.25.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-i18next": "13.5.0",
    "react-router-dom": "6.19.0",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@jest/expect": "29.7.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.1",
    "@testing-library/user-event": "14.5.1",
    "@types/big.js": "6.2.2",
    "@types/cors": "2.8.16",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.8",
    "@types/lodash": "4.14.201",
    "@types/node": "20.9.1",
    "@types/react": "18.2.37",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "esbuild-loader": "4.0.2",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.3",
    "i18next-browser-languagedetector": "7.2.0",
    "i18next-http-backend": "2.4.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-localstorage-mock": "2.4.26",
    "lint-staged": "15.1.0",
    "mini-css-extract-plugin": "2.7.6",
    "prettier": "3.1.0",
    "sonarqube-scanner": "3.3.0",
    "stylelint": "15.11.0",
    "stylelint-config-css-modules": "4.3.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "svgo": "3.0.3",
    "ts-jest": "29.1.1",
    "ts-mockito": "2.6.1",
    "typescript": "5.2.2",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{scss,css}": "stylelint --fix",
    "*.{ts,tsx}": "eslint --fix",
    "*.{ts,tsx,scss,css,js,jsx,json}": "prettier --eslint-config-path .eslintrc.js --write",
    "*.{svg}": "svgo"
  },
  "jestSonar": {
    "reportPath": "build/sonar"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
